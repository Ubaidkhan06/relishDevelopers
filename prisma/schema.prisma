generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Blog {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  content String
  date    DateTime @db.Date
  html    String
  image   String
  title   String
}
